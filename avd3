# Change ownership of laravel.log to nginx:nginx
sudo chown nginx:nginx /var/www/html/Web-Scrapping-Backend/storage/logs/laravel.log

# Also fix permissions
sudo chmod 664 /var/www/html/Web-Scrapping-Backend/storage/logs/laravel.log

